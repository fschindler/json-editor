{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/react-json-editor-e2e/src",
    "projectType": "application",
    "targets": {
        "e2e": {
            "executor": "@nrwl/cypress:cypress",
            "options": {
                "cypressConfig": "packages/react-json-editor-e2e/cypress.config.ts",
                "devServerTarget": "react-json-editor:storybook",
                "testingType": "e2e"
            },
            "configurations": {
                "ci": {
                    "devServerTarget": "react-json-editor:storybook:ci"
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/react-json-editor-e2e/**/*.{js,ts}"]
            }
        }
    },
    "tags": [],
    "implicitDependencies": ["react-json-editor"]
}
